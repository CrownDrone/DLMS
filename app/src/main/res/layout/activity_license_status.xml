<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#b3ffd9"
    tools:context=".LicenseStatus">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="842dp"
        android:layout_height="806dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_crossroads" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="330dp"
        android:layout_height="40dp"
        android:text="License List"
        android:textAlignment="center"
        android:textSize="34dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="match_parent"
        android:layout_height="560dp"
        android:background="#99000000"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.069">

        <TextView
            android:id="@+id/textView19"
            android:layout_width="120dp"
            android:layout_height="32dp"
            android:layout_marginBottom="4dp"
            android:text="Driver's Name"
            android:textColor="@color/white"
            android:textSize="19dp"
            app:layout_constraintBottom_toTopOf="@+id/licenseList"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.017"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.941" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginBottom="4dp"
            android:text="License No."
            android:textColor="@color/white"
            android:textSize="19dp"
            app:layout_constraintBottom_toTopOf="@+id/licenseList"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.107"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.941" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginBottom="4dp"
            android:text="Status"
            android:textColor="@color/white"
            android:textSize="19dp"
            app:layout_constraintBottom_toTopOf="@+id/licenseList"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.545"
            app:layout_constraintStart_toEndOf="@+id/textView20"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.941" />

        <Button
            android:id="@+id/lBack3"
            android:layout_width="114dp"
            android:layout_height="57dp"
            android:text="back"
            android:backgroundTint="#5900b3"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.016"
            app:layout_constraintStart_toStartOf="parent" />

        <ListView
            android:id="@+id/licenseList"
            android:layout_width="409dp"
            android:layout_height="450dp"
            android:divider="#6EA5E8"
            android:dividerHeight="4px"
            app:layout_constraintBottom_toTopOf="@+id/lBack3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>